<% @user.preschools.each do |preschool| %>
	<li><%= preschool.name %> </li>
	<li><%= preschool.address %> </li>
	<li><%= preschool.cost %> </li>
	<li><%= preschool.summary %> </li>
<% end %>

  <form action="/preschools/<%= @preschool.id %>/delete" method="POST">
  	<input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete Preschool">
  </form>

  <form action="/edit/<%= @preschool.id %>" method="POST">
  	<input type="hidden" name="_method" value="PATCH">
  	<input type="submit" value="Edit Preschool">
  </form>